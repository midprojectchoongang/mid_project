<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="db.properties" />
	<typeAliases>
		<typeAlias alias="master" type="model.Master" />
		<typeAlias alias="notice" type="model.Notice" />
		<typeAlias alias="member" type="model.Member" />
		<typeAlias alias="location" type="model.Location" />
		<typeAlias alias="free_board" type="model.Free_board" />
<!--	<typeAlias alias="free_scrap" type="model.Free_scrap" />
		<typeAlias alias="application" type="model.Application" />
		<typeAlias alias="adopt_board" type="model.Adopt_board" />
		<typeAlias alias="adopt_scrap" type="model.Adopt_scrap" />
		<typeAlias alias="shelter" type="model.Shelter" />
 		<typeAlias alias="largecate" type="model.Largecate" />
		<typeAlias alias="smallcate" type="model.Smallcate" />
		<typeAlias alias="comments" type="model.Comments" />  -->
	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="master.xml" />
		<mapper resource="notice.xml" />
 		<mapper resource="location.xml" />
		<mapper resource="member.xml" />
		<mapper resource="free_board.xml" />
<!--	<mapper resource="free_scrap.xml" />
		<mapper resource="application.xml" />
		<mapper resource="adopt_board.xml" />
		<mapper resource="adopt_scrap.xml" />
 		<mapper resource="shelter.xml" />
		<mapper resource="largecate.xml" />
		<mapper resource="smallcate.xml" />
		<mapper resource="comments.xml" />  -->
	</mappers>
</configuration>